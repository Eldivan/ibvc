<p id="notice"><%= notice %></p>

<<div class="jumbotron formulario">
<h1>Adresses</h1>

<table>
  <thead>
    <tr>
      <th>UF</th>
      <th>Cidade</th>
      <th>Bairro</th>
      <th>Quadra</th>
      <th>Conjunto</th>
      <th>Bloco</th>
      <th>Numero</th>
      <th>Condominio</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @adresses.each do |adress| %>
      <tr>
        <td><%= adress.uf %></td>
        <td><%= adress.city %></td>
        <td><%= adress.neighborhood %></td>
        <td><%= adress.quartrain %></td>
        <td><%= adress.set %></td>
        <td><%= adress.block %></td>
        <td><%= adress.number %></td>
        <td><%= adress.townhouse %></td>
        <td><%= link_to 'Mostrar', adress %></td>
        <td><%= link_to 'Editar', edit_adress_path(adress) %></td>
        <td><%= link_to 'Excluir', adress, method: :delete, data: { confirm: 'tem certeza a exclusão ?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
</div>

<%= link_to 'Novo Endereço', new_adress_path %>
