  </div>
-->
  <div class="field">
    <%= form.label :Ativo %>
    <%= form.text_field :active ,  class: 'form-control',placeholder:'Ativo'%>
  </div>

  <div class="botaoenviar">
    <%= form.submit 'Enviar', class: 'btn btn-primary' %>
  </div>
<% end %>
</div>