  <% end %>

  <div class="nome">
    <%= form.label 'Nome' %> <%=form.label:* %>
    <%= form.text_field :name, class: 'form-control',placeholder:'Nome'%>
  </div>

  <div class="field">
    <%= form.label :sexo %>
    <%= form.text_field :sex , class: 'form-control',placeholder:'Sexo' %>
  </div>

  <div class="field">
    <%= form.label :CPF %>
    <%= form.text_field :cpf, class: 'form-control',placeholder:'CPF' %>
  </div>

  <div class="field">
    <%= form.label :Identidade %>
    <%= form.text_field :identity , class: 'form-control',placeholder:'Identidade'%>
  </div>

  <div class="field">
    <%= form.label :Data_Nascimento %>
    <%= form.date_select :date_birth %>
  </div>

  <div class="field">
    <%= form.label :Celular%>
    <%= form.text_field :cell_phone , class: 'form-control',placeholder:'Celular'%>
  </div>

  <div class="field">
    <%= form.label :Email %>
    <%= form.text_field :email , class: 'form-control',placeholder:'Email'%>
  </div>

